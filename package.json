{
  "name": "cross-state-document-builder",
  "version": "1.0.0",
  "description": "Build a document with a Redux reducer, edited across many abstract-state-router states, defined by fsm",
  "main": "es5.js",
  "scripts": {
    "test": "npm run build && tape test.js",
    "build": "browserify . --standalone=\"state-router-redux-ractive\" -o es5.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TehShrike/cross-state-document-builder.git"
  },
  "keywords": [
    "fsm",
    "redux",
    "abstract-state-router",
    "browserify",
    "state"
  ],
  "author": "TehShrike",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/TehShrike/cross-state-document-builder/issues"
  },
  "homepage": "https://github.com/TehShrike/cross-state-document-builder#readme",
  "devDependencies": {
    "babel-preset-es2015": "6.3.13",
    "babelify": "7.2.0",
    "abstract-state-router": "5.10.0",
    "redux": "3.3.1",
    "tape": "4.4.0"
  },
  "dependencies": {
    "dom-value": "1.0.0",
    "fsm": "1.0.2"
  },
  "jsnext:main": "index.js",
  "browser": "index.js",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": "es2015"
        }
      ]
    ]
  }
}
