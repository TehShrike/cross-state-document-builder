{
  "name": "cross-state-document-builder",
  "version": "0.1.3",
  "description": "Build a document with a Redux reducer, edited across many abstract-state-router states",
  "main": "es5.js",
  "scripts": {
    "test": "npm run build:test && node test-build.js",
    "test:browser": "npm run build:test && open test.html",
    "build:test": "browserify test.js -o test-build.js -d",
    "build": "babel index.js --presets es2015 --out-file es5.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TehShrike/cross-state-document-builder.git"
  },
  "keywords": [
    "fsm",
    "redux",
    "abstract-state-router",
    "browserify",
    "state"
  ],
  "author": "TehShrike",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/TehShrike/cross-state-document-builder/issues"
  },
  "homepage": "https://github.com/TehShrike/cross-state-document-builder#readme",
  "devDependencies": {
    "abstract-state-router": "5.10.0",
    "asr-active-state-watcher": "~1.0.0",
    "babel-cli": "6.5.1",
    "babel-preset-es2015": "6.3.13",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "redux": "3.3.1",
    "tap-browser-color": "0.1.2",
    "tape": "4.4.0",
    "tape-catch": "1.0.4"
  },
  "dependencies": {
    "dom-value": "1.0.0"
  },
  "jsnext:main": "index.js",
  "browser": "index.js",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": "es2015"
        }
      ]
    ]
  }
}
